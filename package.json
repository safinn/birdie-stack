{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --no-restart -c \"node server.js\"",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "docker": "docker-compose up -d",
    "generate": "drizzle-kit generate:pg",
    "migrate": "node ./migrations/migrate.js"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.309.0",
    "@aws-sdk/client-ses": "^3.299.0",
    "@remix-run/express": "*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "chokidar": "^3.5.3",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "drizzle-orm": "^0.26.0",
    "esbuild": "^0.17.18",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "morgan": "^1.10.0",
    "postgres": "^3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.4.0",
    "remix-auth-otp": "^2.3.0",
    "tiny-invariant": "^1.3.1",
    "zod-form-data": "^2.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "drizzle-kit": "^0.18.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
